{
	"name": "jasonwynn10/phpstan-plugin-config",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "customiesdevs/customies",
				"version": "1.2.0",
				"source": {
					"url": "https://github.com/CustomiesDevs/Customies.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"psr-4": {
						"customiesdevs\\customies\\": "src/"
					}
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "jasonwynn10/libcustompack",
				"version": "0.1.1",
				"source": {
					"url": "https://github.com/jasonwynn10/libCustomPack.git",
					"type": "git",
					"reference": "master"
				},
				"autoload": {
					"psr-0": {
						"libcustompack\\": "src/libCustomPack/"
					}
				}
			}
		}
	],
	"require": {
		"phpstan/phpstan": "1.4.6",
		"pocketmine/pocketmine-mp": "4.0.0",
		"phpstan/phpstan-strict-rules": "^1.0",
		"phpstan/extension-installer": "^1.0",
		"symfony/filesystem": "^5.4",
		"customiesdevs/customies": "*",
		"jasonwynn10/libcustompack": "*"
	},
	"autoload": {
		"psr-4": {
			"jasonwynn10\\SpectreZone\\": "../../src"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"scripts": {
		"make-baseline": "@php ./vendor/bin/phpstan analyse -b -c phpstan.neon.dist"
	}
}